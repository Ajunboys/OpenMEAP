<S:Envelope xmlns:S="http://schemas.xmlsoap.org/soap/envelope/">
   <S:Body>
      <ns3:connectionOpenResponse xmlns:ns3="com.openmeap.protocol.ws" xmlns:ns2="com.openmeap.protocol.dto">
         <return auth-token="${AUTH_TOKEN}">
            <ns2:update type="${UPDATE_TYPE}" update-url="${UPDATE_URL}" storage-needs="${STORAGE_NEEDS}" version-identifier="${VERSION_ID}" install-needs="${INSTALL_NEEDS}">
               <ns2:hash value="${HASH}" algorithm="${HASH_ALG}"/>
            </ns2:update>
         </return>
      </ns3:connectionOpenResponse>
   </S:Body>
</S:Envelope>